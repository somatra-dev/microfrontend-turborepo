{
  "name": "@repo/redux",
  "version": "0.0.0",
  "private": true,
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./provider": {
      "types": "./src/provider.tsx",
      "default": "./src/provider.tsx"
    },
    "./store": {
      "types": "./src/store.ts",
      "default": "./src/store.ts"
    },
    "./hook": {
      "types": "./src/hook.ts",
      "default": "./src/hook.ts"
    },
    "./feature/*": {
      "types": "./src/feature/*/index.ts",
      "default": "./src/feature/*/index.ts"
    }
  },
  "scripts": {
    "lint": "eslint . --max-warnings 0",
    "check-types": "tsc --noEmit"
  },
  "devDependencies": {
    "@repo/eslint-config": "*",
    "@repo/typescript-config": "*",
    "@types/node": "^22.15.3",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "eslint": "^9.39.1",
    "typescript": "5.9.2"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.11.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-redux": "^9.2.0"
  }
}
